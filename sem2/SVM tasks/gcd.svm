ldc a
ldc b
st 0 ;число0 = b
st 1 ;число1 = а
ld 1 
ld 0 ;на стеке а и b, на вершине стека b
step: mod ;на стеке остаток от деления делимого на делитель
ld 0 
st 1 ;делитель становится делимым
st 0 ;остаток становится делителем
ld 1 
ld 0
ld 0 ;на стеке делимое, делитель и снова делитель (который пока еще и остаток)
br step ;если остаток от деления не 0, то step (на стеке новые делимое и делитель)
add ;если остаток 0, то на стеке НОД и 0
hlt
